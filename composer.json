{
    "name": "unk/praesidium",
    "description": "Sistema de permisos y roles para laravel 8.0",
    "type": "library",
    
    "authors": [
        {
            "name": "Unknowns",
            "email": "unknowns0074@gmail.com"
        }
    ],
    
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },
    
    "autoload": {
        "psr-4": {
            "UNK\\Praesidium\\": "src/"
        },

        "classmap": [
            "src/database/seeders"
        ]
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "UNK\\Praesidium\\PraesidiumServiceProvider"
            ],
            
            "aliases": {
                "Praesidium": "UNK\\Praesidium\\Facades\\PraesidiumFacade"
            }
        }
    },
    
    "minimum-stability": "dev"
}
